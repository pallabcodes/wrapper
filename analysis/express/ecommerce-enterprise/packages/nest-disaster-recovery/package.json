    "@nestjs/swagger": "^7.4.2",
    "@ecommerce-enterprise/nest-zod": "workspace:*",
    "rxjs": "^7.8.2",
    "ioredis": "^5.7.0",
    "aws-sdk": "^2.1691.0",
    "azure-storage": "^2.10.3",
    "google-cloud-storage": "^2.0.2",
    "cron": "^3.1.6",
    "moment": "^2.30.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
