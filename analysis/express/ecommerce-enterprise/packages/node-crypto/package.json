{
  "name": "@ecommerce-enterprise/node-crypto",
  "version": "1.0.0",
  "description": "Enhanced Node.js crypto module with enterprise features, audit trails, and performance monitoring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:addon && tsc",
    "build:addon": "node-gyp rebuild",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "demo": "ts-node demo/run-demo.ts",
    "clean": "rimraf dist build",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "crypto",
    "encryption",
    "security",
    "enterprise",
    "audit",
    "compliance",
    "performance",
    "node-addon",
    "typescript"
  ],
  "author": "Ecommerce Enterprise Team",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^7.0.0",
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "rimraf": "^5.0.0",
    "node-gyp": "^10.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "gypfile": true,
  "files": [
    "dist/**/*",
    "build/**/*",
    "binding.gyp",
    "src/**/*.cc",
    "src/**/*.h",
    "README.md"
  ]
}
